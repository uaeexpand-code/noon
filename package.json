{
  "name": "uae-calendar-server",
  "version": "1.0.0",
  "description": "Server for the UAE Seller's Smart Calendar",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "build": "esbuild index.tsx --bundle --outfile=bundle.js --jsx=automatic --format=esm --external:react --external:react/* --external:react-dom/* --external:@google/genai",
    "start": "npm run build && node server.js"
  },
  "dependencies": {
    "@google/genai": "^1.28.0",
    "express": "^4.19.2",
    "esbuild": "^0.21.4"
  }
}